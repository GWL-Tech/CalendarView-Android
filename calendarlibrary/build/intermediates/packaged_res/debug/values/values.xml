    <style name="AppTheme" parent="Theme.AppCompat">
        <item name="customColor">?attr/colorDays</item>
        <item name="android:textColorPrimary">?attr/colorDays</item>
    </style>
    <style name="AppTheme.AnotherColor" parent="Theme.AppCompat">
        <item name="colorDays">@color/white</item>
    </style>
    <style name="CurrentDateBackgroundImg" parent="@android:style/Theme.NoTitleBar">
        <item name="currentDateBackgroundImg">@drawable/reminder</item>
    </style>
    <style name="CustomActionBar">
        <!-- title text color -->
        <item name="android:textColorPrimary">?attr/colorDays</item>
    </style>
    <style name="EventDrawable" parent="@android:style/Theme.NoTitleBar">
        <item name="eventImageDrawable">@drawable/circle_dot</item>
    </style>
    <style name="Theme.CalendarLibrary" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/purple_500</item>
        <item name="colorPrimaryVariant">@color/purple_700</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" ns1:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>
    <declare-styleable name="CalendarView">
        <attr format="string" name="dateFormat"/>
        <attr format="color" name="colorMonth"/>
        <attr format="color" name="colorDays"/>
        <attr format="color" name="colorDates"/>
        <attr format="color" name="disableColorDates"/>
        <attr format="color" name="selectedColorDates"/>
        <attr format="reference" name="currentDateBackgroundImg"/>
        <attr format="reference" name="eventImageDrawable"/>
        <attr format="enum" name="eventIconPosition">
            <enum name="TOP" value="0"/>
            <enum name="BOTTOM" value="1"/>
            <enum name="LEFT" value="2"/>
            <enum name="RIGHT" value="3"/>
            <enum name="TOP_LEFT" value="4"/>
            <enum name="TOP_RIGHT" value="5"/>
            <enum name="BOTTOM_LEFT" value="6"/>
            <enum name="BOTTOM_RIGHT" value="7"/>
        </attr>
    </declare-styleable>
